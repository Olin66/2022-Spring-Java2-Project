<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.cs209.project.mapper.WordMapper">
    <select id="selectIssueWord" resultType="com.cs209.project.entity.IssueWord">
        SELECT *
        FROM issue_word;
    </select>
    <select id="selectQuestionWord" resultType="com.cs209.project.entity.QuestionWord">
        SELECT *
        FROM question_word;
    </select>
</mapper>